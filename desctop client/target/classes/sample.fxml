            <Label maxHeight="40.0" maxWidth="500.0" minHeight="40.0" minWidth="500.0" prefHeight="40.0" prefWidth="500.0" text="Actually connected devices" />
            <TableView fx:id="devicesTableView" maxHeight="560.0" maxWidth="500.0" minHeight="560.0" minWidth="500.0" prefHeight="560.0" prefWidth="500.0" />
